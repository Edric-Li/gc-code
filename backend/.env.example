# 服务器配置
NODE_ENV=development
PORT=5555
API_PREFIX=/api

# 数据库配置
DATABASE_URL=postgresql://postgres:password@localhost:5432/gc_code_portal

# JWT 配置
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=1h

# 前端地址
FRONTEND_URL=http://localhost:3000

# Azure AD 配置
AZURE_AD_CLIENT_ID=your-azure-ad-client-id
AZURE_AD_TENANT_ID=your-azure-ad-tenant-id
AZURE_AD_CLIENT_SECRET=your-azure-ad-client-secret
AZURE_AD_REDIRECT_URI=http://localhost:5555/api/auth/azure/callback

# Google OAuth (可选)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5555/api/auth/google/callback

# GitHub OAuth (可选)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_REDIRECT_URI=http://localhost:5555/api/auth/github/callback

# External Claude Relay Service API
RELAY_API_BASE_URL=https://console.gccode.cn
RELAY_API_USERNAME=your-relay-username
RELAY_API_PASSWORD=your-relay-password
